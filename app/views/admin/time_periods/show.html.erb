<%
page_title t('.title',
             label: t('activerecord.attributes.time_period.title'),
             label_value: @time_period.title)
%>

<div class='actions-container'>
  <%= render partial: 'shared/show_actions',
        locals: {
          object: @time_period,
          index_path: admin_time_periods_path,
          show_path: admin_time_period_path,
          edit_path: edit_admin_time_period_path
                }
  %>
</div>

<dl class="dl-horizontal">
  <dt><strong><%=t 'activerecord.attributes.time_period.label' %>:</strong></dt>
  <dd><%= @time_period.label %></dd>
  <dt><strong><%=t 'activerecord.attributes.time_period.order' %>:</strong></dt>
  <dd><%= @time_period.order %></dd>
  <dt><strong><%=t 'activerecord.attributes.time_period.is_published' %>:</strong></dt>
  <dd><%= @time_period.is_published %></dd>
</dl>
